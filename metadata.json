  "dependencies": [
    {
      "name": "puppet/archive",
      "version_requirement": ">= 3.0.0 < 9.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.22.0 < 10.0.0"
    },
    {
      "name": "puppet/logrotate",
      "version_requirement": ">= 3.0.0 < 10.0.0"
    }
  ]
