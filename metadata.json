  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.0.0 <6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppet/archive",
      "version_requirement": ">= 1.3.0 < 3.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.2.0 < 5.0.0"
